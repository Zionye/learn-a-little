    "autoprefixer": "10.4.16",
    "axios": "^1.5.1",
    "clsx": "^2.0.0",
    "eslint-config-next": "14.0.1",
    "next": "14.0.1",
    "next-auth": "^4.24.5",
    "next-themes": "^0.2.1",
    "postcss": "8.4.30",
    "prisma": "^5.4.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
